<%= @stock_symbol%>
<%= @shares_sum.values%>
<%= @shares_ave.values%>

<%= render '/portfolios/sell_calculation', @transaction%>

<%= @current_price%>
<%= @count_shares%>

<%=@price_at_purchase%>
<%= render '/portfolios/sell_form', @transaction%>



<div class= "portfolio_table">

<table class ="portfolio_detail">
  <tbody>
    <% @stock_symbol.each do |sym| %>
        <tr>
          <td>
            <p> <%= sym %>
            </p>
          </td>
        </tr>
    <% end %>
  </tbody>
</table>

<table class="portfolio_detail" >
  <tbody>
    <% @sum.each do |sum| %>
      <tr>
          <td>
            <p> <%= sum %></p>
          </td>
        </tr>
        </tr>
    <% end %>
    
  </tbody>
</table>

<table class="portfolio_detail" >
  <tbody>
    <% @ave.each do |ave| %>
      <tr>
          <td>
            <p> <%= ave %></p>
          </td>
        </tr>
        </tr>
    <% end %>
    
  </tbody>
</table>
</div>


