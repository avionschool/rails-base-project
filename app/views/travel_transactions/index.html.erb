<br/><br/><br/><br/>
<table class="table table-striped text-center">
  <thead class="thead-dark">
    <tr>
      <th scope="col" class="Package">Package Name</th>
      <th scope="col">Tourist</th>
      <th scope="col">Agency</th>
      <th scope="col">Date Availed</th>
      <th scope="col">Start Date</th>
      <th scope="col">End Date</th>
    </tr>
  </thead>
  <tbody>
  <% @travel_transactions.each do |transaction|%>
    <tr>
      <td><%= transaction.tourist_tour.tour.name%></td>
      <td><%= transaction.tourist_tour.tourist.email%></td>
      <td><%= transaction.agency.email%></td>
      <td><%= transaction.created_at.strftime('%b %d, %Y')%></td>
         <%if !transaction.tourist_tour.start_date.nil?%>
            <td><%= transaction.tourist_tour.start_date.strftime('%b %d, %Y')%></td>
        <%end%>
        <%if !transaction.tourist_tour.end_date.nil?%>
            <td><%= transaction.tourist_tour.end_date.strftime('%b %d, %Y')%></td>
        <%end%>
    </tr>
    <%end%>
  </tbody>
</table>

<div class="flickr_pagination">
    <div class="page_info">
      <%= page_entries_info @travel_transactions %>
    </div>
    <%= will_paginate @travel_transactions, :container => false %>
  </div>